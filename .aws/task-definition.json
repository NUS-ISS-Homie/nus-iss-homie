{
  "family": "homie",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "homie-backend",
      "image": "[AWS ACC ID].dkr.ecr.ap-southeast-1.amazonaws.com/homie-ecr-repo",
      "cpu": 256,
      "memory": 512,
      "portMappings": [
        {
          "containerPort": 8000,
          "hostPort": 8000
        }
      ],
      "essential": true
    }
  ],
  "taskRoleArn": "arn:aws:iam::[AWS ACC ID]:role/ecsTaskExecutionRole",
  "executionRoleArn": "arn:aws:iam::[AWS ACC ID]:role/ecsTaskExecutionRole",
  "volumes": [],
  "placementConstraints": [],
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512"
}
